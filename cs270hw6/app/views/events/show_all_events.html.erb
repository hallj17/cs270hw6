<%# table, with event, start and end th %>
<table>
    <tr>
        <th> Event</th>
        <th> Start</th>
        <th> End</th>
    </tr>
    
<%# create views, remember end tag %>    
<% @events.each do |e| %>
    <tr>
        <td> <%=e.name %></td>
        <td> <%=e.start_date %></td>
        <td> <%=e.end_date %></td>
    </tr>
    
<% end %>
    
</table>